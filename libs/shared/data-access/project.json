{
  "name": "data-access",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/data-access/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/shared/data-access",
        "main": "libs/shared/data-access/src/index.ts",
        "tsConfig": "libs/shared/data-access/tsconfig.lib.json",
        "assets": [
          "libs/shared/data-access/*.md"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/shared/data-access/jest.config.cts"
      }
    },
    "db-heal": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node libs/shared/data-access/src/lib/scripts/heal.ts"
      }
    },
    "db-seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node libs/shared/data-access/src/lib/scripts/seed-admin.ts"
      }
    }
  }
}